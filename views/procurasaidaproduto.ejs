<%- include('partials/header') %>

<style>
  #rowMestra {
    max-width: 99%;
    margin-left: 1%
  }

  #titulo {
    text-align: center;
  }

  .validadeInput {
    width: 100%;
    height: 57px;
  }

  #validadeDescritivo {
    font-size: 14px;
    padding-top: 0;
    padding-left: 2px;
  }

  .validadeDiv {
    border-color: gray;
    background-clip: padding-box;
    opacity: .50;
  }
</style>

<form action="/saidaproduto" method="get" class='mb-3'>
  <div class="row" id='rowMestra'>
    <div class="row" id='descritivoCadastro'>
      <div class="col-lg-12 mt-2" id='titulo'>
        <h3 class='descritivoCadastro'>Sa√≠da de Produto</h3>
      </div>
    </div>
    <div class="row" id='rowTerceira'>
      <div class="col-lg">
        <div class="form-floating">
          <select class="form-select" id="classificacaoSelect" aria-label="Floating label" name="nome">
            <%  produto.forEach(function(produtos){ %>
            <option class='valor'><%=produtos.nome%></option>
            <% }) %>
          </select>
          <label for="classificacaoSelect">Produto:</label>
        </div>
      </div>
    </div>
  </div>
  <button type="submit" id='botaoSalva' class="btn btn-success mt-2" style='margin-left: 46%;'>Procurar</button>
</form>


<%- include('partials/footer') %>
